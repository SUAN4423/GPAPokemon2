{
  "name": "GPAPokemon",
  "version": "1.00",
  "manifest_version": 2,
  "description": "GPAポケモンを表示させます",
  "permissions": ["http://*/*", "https://*/*"],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}